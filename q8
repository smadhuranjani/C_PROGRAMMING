# include<stdio.h>
int main()
{
	int num,t,n,i,nt,st;
	printf("enter the total number of user:");
	scanf("%d",&n);
	printf("enter the number of teaching staff:");
	scanf("%d",&t);
	if(n>0 && n>t+t/3)
	{
	nt=t/3;
	st=n-(t+nt);
	printf("students users:%d",st);
	}
	else
{
	printf("Invalid input");
	}
	}
