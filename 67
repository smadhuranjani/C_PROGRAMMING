#include <stdio.h>
int main()
{
char str[100];
int i,lower_case,upper_case,digits,special;
i = lower_case=upper_case= digits = special = 0;
printf("Enter any String : ");
gets(str);
while (str[i] != '\0')
{
if( (str[i] >= 'a' && str[i] <= 'z') )
{
lower_case++;
}
else if((str[i] >= 'A' && str[i] <= 'Z'))
{
upper_case++;
}
else if (str[i] >= '0' && str[i] <= '9')
{
digits++;
}
